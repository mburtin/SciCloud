{
  "name": "scicloud",
  "version": "0.1.0",
  "type": "module",
  "description": "SciCloud - Cloud services for scientific research",
  "scripts": {
    "dev": "concurrently \"pnpm supabase:start\" \"pnpm supabase:functions\" \"pnpm web:dev\"",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "web:preview": "pnpm --filter web preview",
    "web:test": "pnpm --filter web test",
    "supabase:start": "cd apps/supabase && supabase start",
    "supabase:stop": "cd apps/supabase && supabase stop",
    "supabase:status": "cd apps/supabase && supabase status",
    "supabase:functions": "sleep 5 && cd apps/supabase && supabase functions serve --no-verify-jwt",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "pnpm --filter web type-check"
  },
  "keywords": [
    "scientific research",
    "data analysis",
    "project management",
    "cloud services"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0"
  }
}
