{
  "name": "scicloud",
  "version": "0.1.0",
  "type": "module",
  "description": "SciCloud - Cloud services for scientific research",
  "scripts": {
    "dev": "concurrently \"pnpm supabase:start\" \"pnpm supabase:functions\" \"pnpm web:dev\"",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "web:preview": "pnpm --filter web preview",
    "supabase:start": "cd apps/supabase && supabase start",
    "supabase:stop": "cd apps/supabase && supabase stop",
    "supabase:reset": "cd apps/supabase && supabase db reset",
    "supabase:status": "cd apps/supabase && supabase status",
    "supabase:functions": "sleep 5 && cd apps/supabase && supabase functions serve --no-verify-jwt",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "pnpm --filter web type-check",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "keywords": [
    "scientific research",
    "data analysis",
    "project management",
    "cloud services"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "pinia": "^3.0.3",
    "vite": "^6.0.7",
    "vitest": "^3.1.1",
    "vue": "^3.5.18"
  }
}
